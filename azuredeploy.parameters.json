      "value": "\\\\<AZUREFILESURI>.file.core.windows.net\\<installpath>"
    },
    "location": {
      "value": "<LOCATION>"
    },
    "SQLAgentUserName": {
      "value": "<DOMAIN\\USERNAME>"
    },
    "SQLAgentPassword": {
      "value": "<SQLAGENTPASSWORD>"
    },
    "SQLSAAccountPassword": {
      "value": "<SQLSAACCOUNTPASSWORD>"
    },
    "SQLServiceUserName": {
      "value": "<DOMAIN\\USERNAME>"
    },
    "SQLServicePassword": {
      "value": "<SQLSERVICEPASSWORD>"
    },
    "Features": {
      "value": "SQLENGINE,FULLTEXT" //SQL Features you want installed 
    },
    "UpdateSource": {
      "value": "MU"
    },
    "UpdateEnabled": {
      "value": "False"
    },
    "InstallSharedDir": {
      "value": "C:\\Program Files\\Microsoft SQL Server"
    },
    "InstallSharedWOWDir": {
      "value": "C:\\Program Files (x86)\\Microsoft SQL Server"
    },
    "SQLInstanceName": {
      "value": "MSSQLSERVER"
    },
    "SQLInstanceDir": {
      "value": "C:\\Microsoft SQL Server\\MSSQLSERVER"
    },
    "SecurityMode": {
      "value": "SQL"
    },
    "SQLSysAdminAccounts": {
      "value": "BUILTIN\\Administrators"
    }
  }
}