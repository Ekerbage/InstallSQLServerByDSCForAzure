{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {

    "vmName": {
      "value": "<VMNAME>"
    },
    "vmSize": {
      "value": "Standard_DS3_v2"
    },
    "assetLocation": {
      "value": "https://petedscutil.blob.core.windows.net/scripts/"
    },
    "AdminUserName": {
      "value": "<ADMINUSERNAME>"
    },
    "AdminPassword": {
      "value": "<PASSWORD>"
    },
    "DomainUserName": {
      "value": "<DOMAIN\\USERNAME>"
    },
    "DomainPassword": {
      "value": "<DOMAIN_PASSWORD>"
    },
    "existingDomainName": {
      "value": "<DOMAINNAME>"
    },
    "existingOUPath": {
      "value": ""
    },
    "existingVirtualNetworkName": {
      "value": "<VNETNAME>"
    },
    "existingVirtualNetworkResourceGroup": {
      "value": "<VNET_RESOURCE_GROUP>"
    },
    "storageAccountUri": {
      "value": "https://storageaccounturi.blob.core.windows.net"
    },
    "bootdiagnosticsstorageAccountUri": {
      "value": "https://storageaccounturi.blob.core.windows.net"
    },
    "windowsOSVersion": {
      "value": "2012-R2-Datacenter"
    },
    "subnetName": {
      "value": "default"
    },
    "FileShareUserName": {
      "value": "<AZUREFILESSHAREUSERNAME>"
    },
    "FileSharePassword": {
      "value": "<STORAGEACCOUNTFILESHAREPASSWORD>"
    },
    "InstallDir": {
      "value": ""
    },
